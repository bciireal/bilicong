{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "bilisync",
  "version": "0.1.1",
  "identifier": "top.bciireal.bilisync",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "bilisync",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "default-src 'self'; img-src 'self' data: *.hdslb.com"
    }
  },
  "bundle": {
    "active": false
  }
}
